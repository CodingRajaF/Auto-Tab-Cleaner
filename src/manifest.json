{
    "manifest_version": 3,
    "name": "Auto Tab Cleaner",
    "version": "1.0",
    "description": "一定時間放置されたタブを自動で閉じる",
    "permissions": [
        "tabs",
        "storage",
        "alarms"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html"
}
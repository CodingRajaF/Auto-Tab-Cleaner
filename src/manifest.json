{
    "manifest_version": 3,
    "name": "Auto-Tab-Cleaner",
    "version": "0.1.0",
    "description": "30分放置されたタブを自動で閉じる(MVP)",
    "permissions": [
        "tabs",
        "alarms"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "actions": {
        "default_title": "Auto Tab Cleaner"
    }
}